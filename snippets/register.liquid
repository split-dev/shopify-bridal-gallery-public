{{ 'customer.css' | asset_url | stylesheet_tag }}

<div id="registerModal" class="register-modal">
	<div class="register-modal__content">
		<span id="closer" class="closer">
			<svg  xmlns="http://www.w3.org/2000/svg" width="28" height="28" viewBox="0 0 28 28" fill="none">
			<g clip-path="url(#clip0_355_6334)">
			<path d="M15.1004 14L21.5482 7.55224C21.6756 7.40345 21.7422 7.21206 21.7347 7.01631C21.7271 6.82056 21.646 6.63487 21.5074 6.49636C21.3689 6.35784 21.1832 6.27669 20.9875 6.26913C20.7917 6.26157 20.6003 6.32815 20.4516 6.45557L14.0038 12.9033L7.556 6.44779C7.40954 6.30133 7.2109 6.21906 7.00378 6.21906C6.79665 6.21906 6.59801 6.30133 6.45155 6.44779C6.3051 6.59425 6.22282 6.79289 6.22282 7.00002C6.22282 7.20714 6.3051 7.40578 6.45155 7.55224L12.9071 14L6.45155 20.4478C6.37014 20.5175 6.30401 20.6033 6.25732 20.6998C6.21064 20.7963 6.1844 20.9014 6.18027 21.0085C6.17613 21.1156 6.19418 21.2225 6.23328 21.3223C6.27238 21.4221 6.3317 21.5127 6.40749 21.5885C6.48329 21.6643 6.57394 21.7236 6.67375 21.7627C6.77356 21.8018 6.88037 21.8199 6.98749 21.8157C7.0946 21.8116 7.1997 21.7854 7.2962 21.7387C7.39269 21.692 7.4785 21.6259 7.54822 21.5445L14.0038 15.0967L20.4516 21.5445C20.6003 21.6719 20.7917 21.7385 20.9875 21.7309C21.1832 21.7233 21.3689 21.6422 21.5074 21.5037C21.646 21.3652 21.7271 21.1795 21.7347 20.9837C21.7422 20.788 21.6756 20.5966 21.5482 20.4478L15.1004 14Z" fill="black"/>
